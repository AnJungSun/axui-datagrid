{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",
    "declaration": false,
    "noImplicitAny": false,
    "sourceMap": true,
    "jsx": "react",
    "noLib": false,
    "suppressImplicitAnyIndexErrors": true,
  },
  "compileOnSave": false,
  "exclude": [
    "node_modules", "src"
  ],
  "awesomeTypescriptLoaderOptions": {
    "useCache": true
  }
}
